<div class="applicant-table">
    <table>
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Company Name</th>
            <th scope="col">Job Designation</th>
            <th scope="col">Job Location</th>
            <th scope="col">View Job</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0;  i < jobs.length;i++){ %>
            <tr>
              <td><%= i+1 %></td>
              <td><%= jobs[i].companyName %></td>
              <td><%= jobs[i].jobDesignation %></td>
              <td><%= jobs[i].jobLocation %></td>
              <td><a href="/viewDetail/<%= jobs[i].jobId %>"><button class="button">View</button></a>
            </tr>
         <% } %>
         
        </tbody>
      </table>
   </div>